<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>prueba_concepto</servlet-name>
		<servlet-class>ar.com.cdt.prueba.DemoServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>prueba_concepto_param</servlet-name>
		<servlet-class>ar.com.cdt.prueba.DemoServletParam</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>prueba_concepto</servlet-name>
		<url-pattern>/prueba</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>prueba_concepto_param</servlet-name>
		<url-pattern>/nombre/*</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
